{% extends 'base.html.twig' %}

{% block body %}
<h1>User</h1>

<table>
    <tbody>
    <tr>
        <th>Article Id</th>
        <td>{{ article.id }}</td>
    </tr>
    <tr>
        <th>User Id</th>
        <td>{{ article.userId }}</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>{{ article.name }}</td>
    </tr>
    <tr>
        <th>Text</th>
        <td>{{ article.text }}</td>
    </tr>
    <tr>
        <th>Created At</th>
        <td>{{ article.createdAt|date('Y-m-d') }}</td>
    </tr>
    <tr>
        <th>Updated At</th>
        <td>{{ article.updatedAt|date('Y-m-d') }}</td>
    </tr>
    <tr>
        <th>Is Deleted</th>
        <td>{{ article.isDeleted }}</td>
    </tr>
    <tr>
        <th>Is Active</th>
        <td>{{ article.isActive }}</td>
    </tr>
    </tbody>
</table>

{% endblock %}